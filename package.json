{
    "name": "nerdery-rest-module",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "git@github.com:irenehl/nerdery-rest-module.git",
    "author": "irenehl <26irenelopez@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/src/main.js",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/main.js\"",
        "lint": "eslint --ext .js,.ts .",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "test": "jest", 
        "faker": "ts-node --compiler-options '{\"module\": \"CommonJS\"}' \"prisma/seed.ts\""
    },
    "dependencies": {
        "@prisma/client": "^4.14.0",
        "express": "^4.18.2",
        "prisma": "^4.14.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.0.0",
        "@types/express": "^4.17.17",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.59.5",
        "concurrently": "^8.0.1",
        "eslint": "^8.0.1",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "faker": "^6.6.6",
        "jest": "^29.5.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "typescript": "*"
    }
}
